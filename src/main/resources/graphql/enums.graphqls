enum MemberRole {
    ADMIN
    USER
    INSPECTOR
}

enum ApprovementType {
    MEMBERS_CONFIRM
    FORM
}

enum AnswerType {
    MULTIPLE
    SINGLE
}

enum AnswerStatus {
    # The answer was created, but not sent
    CREATED
    # The answer has been sent for review
    SENT
    # The answer was reviewed with failed result
    FAILED
    # The answer was reviewed with passed result
    PASSED
}

enum ConfirmationStatus {
    WAITING
    APPLIED
    REJECTED
}